<header>
    <nav class="clearfix">
<% if @override_title_logo %>
        <h1><a href="<%= book_path(@book) %>"><%= image_tag @book.logo_url, alt: @book.title %></a></h1>
<% else %>
        <h1><a href="<%= root_path %>"><%= image_tag asset_path("spineless_logo.png"), alt: "spineless" %></a></h1>
<% end %>
<% if logged_in? %>
        <p><%= link_to "Profile", current_user %></p>
        <p><%= link_to "Log out", logout_path, method: :delete %></p>
<% else %>
        <p><%= link_to "Login", login_path %></p>
        <p><%= link_to "Sign up", signup_path %></p>
<% end %>
    </nav>
</header>